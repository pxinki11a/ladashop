<Window x:Class="ladashopq.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ladashopq"
        mc:Ignorable="d"
        Title="AdminWindow" Height="520" Width="920" WindowStartupLocation="CenterScreen" Background="#D5CFE1" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Text="Окно администратора"/>
        
        


        <Frame Grid.Row="2" NavigationUIVisibility="Hidden" Name="AdminFrame" Navigated="AdminFrame_Navigated"/>
        <Button Grid.Row="1" Content="Вернуться ко входу" FontSize="12" Background="white" HorizontalAlignment="Right"  Height="35" Width="155" Click="ButtonBack_Click"/>
        <Button Grid.Row="1" HorizontalAlignment="Center" Margin=" 0 0 40 0" Content="Добавить товар" Name="BtnAdd" Click="BtnAdd_Click"/>
    </Grid>
</Window>

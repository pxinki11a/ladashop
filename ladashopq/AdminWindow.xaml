<Window x:Class="ladashopq.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ladashopq"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Text="Окно администратора"/>
        <ComboBox Name="SelectTable" Grid.Row="1" SelectionChanged="ComboBox_SelectionChanged" Width="169">
            <ComboBoxItem Content="Товары"/>
            <ComboBoxItem Content="Категории"/>
            <ComboBoxItem Content="Роли"/>
            <ComboBoxItem Content="Модели"/>
            <ComboBoxItem Content="Пользователи"/>
            <ComboBoxItem Content="Поставщики"/>
        </ComboBox>
        <Frame Grid.Row="2" NavigationUIVisibility="Hidden" Name="MainFrame" Navigated="MainFrame_Navigated"/>
        <TextBox x:Name="nameFilter" Height="36" Margin="604,15,21,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Grid.Row="1"/>
        <TextBlock Grid.Row="1" Text="Поиск:" HorizontalAlignment="Left" Margin="534,23,0,316" Grid.RowSpan="2"></TextBlock>
    </Grid>
</Window>
